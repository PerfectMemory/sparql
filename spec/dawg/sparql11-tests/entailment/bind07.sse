(prefix ((: <http://example.org/>)
         (owl: <http://www.w3.org/2002/07/owl#>)
         (rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>))
  (project (?s ?p ?o ?z)
    (join
      (bgp
        (triple ?s ?p ?o)
        (triple ?p rdf:type owl:DatatypeProperty)
      )
      (union
        (extend ((?z (+ ?o 1)))
          (table unit))
        (extend ((?z (+ ?o 2)))
          (table unit))))))
