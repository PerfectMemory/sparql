(prefix ((: <http://example.org/>)
         (owl: <http://www.w3.org/2002/07/owl#>)
         (rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>))
  (project (?s ?p ?o ?z)
    (filter (= ?z 3)
      (extend ((?z (+ ?o 1)))
        (bgp
          (triple ?s ?p ?o)
          (triple ?p rdf:type owl:DatatypeProperty)
        )))))
